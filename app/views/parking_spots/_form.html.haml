= form_for @parking_spot do |f|
  - if @parking_spot.errors.any?
    #error_explanation
      %h2= "#{pluralize(@parking_spot.errors.count, "error")} prohibited this parking_spot from being saved:"
      %ul
        - @parking_spot.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :number
    = f.number_field :number
  .actions
    = f.submit 'Save'
